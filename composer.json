{
  "name": "colinhdev/cplot",
  "minimum-stability": "dev",
  "require": {
    "pocketmine/pocketmine-mp": "dev-major-next as 4.0.0",
    "sof3/await-generator": "^3.4.3",
    "sof3/libasynql": "4.1.6",
    "sofe/capital": "^0.1.2",
    "cooldogedev/bedrockeconomy": "^2.0.8",
    "colinhdev/libasyncevent": "^1.0.0",
    "ext-zlib": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10.3",
    "phpstan/phpstan-strict-rules": "^1.5.0",
    "phpstan/extension-installer": "^1.2.0"
  },
  "repositories": {
    "capital": {
      "type": "vcs",
      "url": "https://github.com/SOF3/Capital"
    },
    "bedrockeconomy": {
      "type": "package",
      "package": {
        "name": "cooldogedev/bedrockeconomy",
        "version": "2.0.8",
        "source": {
          "type": "git",
          "url": "https://github.com/cooldogedev/BedrockEconomy",
          "reference": "main"
        }
      }
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
